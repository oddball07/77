{
  "format": "page",
  "id": "projects",
  "label": "Projects",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "uid": {
        "type": "UID",
        "config": {
          "label": "project"
        }
      },
      "title": {
        "type": "Text",
        "config": {
          "label": "Title",
          "placeholder": "Project Name"
        }
      },
      "description": {
        "type": "StructuredText",
        "config": {
          "label": "Description",
          "placeholder": "Project Description",
          "allowTargetBlank": true,
          "multi": "paragraph,preformatted,strong,em,hyperlink,list-item,o-list-item"
        }
      },
      "project_link": {
        "type": "Link",
        "config": {
          "label": "Project Link",
          "placeholder": "Project Link",
          "select": null,
          "allowTargetBlank": true,
          "allowText": true,
          "repeat": false
        }
      },
      "slices": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {
            "project_images": {
              "type": "SharedSlice"
            }
          }
        }
      }
    }
  }
}
